        sprite[RIGHT, MOVE] = spr_knight_right;
        sprite[UP, MOVE] = spr_knight_up;
        sprite[DOWN, MOVE] = spr_knight_down;
        sprite[LEFT, MOVE] = spr_knight_left;
        sprite[RIGHT, ATTACK] = spr_knight_right;
        sprite[UP, ATTACK] = spr_knight_up;
        sprite[DOWN, ATTACK] = spr_knight_down;
        sprite[LEFT, ATTACK] = spr_knight_left;
        break;
    }

sprite_index = sprite[face, movement];
    if (obj_player_stats.hp &lt;= 0) {
    instance_destroy();
}


